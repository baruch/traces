TRACES_ROOT_DIR:=$(abspath $(dir $(lastword $(MAKEFILE_LIST)))..)
CCWRAP=$(TRACES_ROOT_DIR)/ccwrap.py
LDWRAP=$(TRACES_ROOT_DIR)/ldwrap.py
CC=$(CCWRAP) gcc
CXX=$(CCWRAP) g++
CXXFLAGS=-I../include
LD=$(LDWRAP) ld
LDFLAGS=-L$(TRACES_ROOT_DIR)
all: demo

demo: demo.o
	$(CC) $(LDFLAGS) demo.o ../libtrace/.libs/libtrace_user.a -lrt -L. -o demo

clean:
	rm demo.o demo
